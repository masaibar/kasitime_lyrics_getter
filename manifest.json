{
  "name": "歌詞ゲッター from 歌詞タイム",
  "version": "1.0",
  "manifest_version": 2,

  "description" : "歌詞タイムからクリップボードへ歌詞をコピーします",

  "icons" : {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "permissions": [
  	"tabs", "http://*/*"
  ], 

  "background": {
  	"scripts": ["background.js"]
  },

  "page_action": {
	"default_icon": "icons/icon19.png",
	"default_title": "歌詞ゲッター from 歌詞タイム",
	"default_popup": "popup.html"
  }
}
